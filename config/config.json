{
  "run_symbol": {
    "symbol": "EURUSD",
    "timeframe": "15m",
    "start_date": "2015-01-01",	
    "end_date": "2025-04-21",
    "source": "mt5"
  },

  "scan_symbols": [
    "EURUSD", "BTCUSD"
  ],
  "scan_symbols-YFINANCE": ["DIS", "GOOG", "META", "NFLX", "SNAP", "VZ", "AMZN", "F", 
		  "GM", "MCD", "NKE", "TSLA", "KO", "PG", "TAP", "WMT", "CVX",
		  "SUB", "XOM", "BAC", "C", "JPM", "V", "JNJ", "PFE", "UNH",
		  "BA", "CAT", "RTX", "AAPL", "AMD", "HPQ", "IBM", "INTC", "MSFT",
		  "NVDA", "ORCL", "CL=F", "GC=F", "SI=F", "PL=F", "HG=F", "PA=F",
		  "BTC-USD"],

  "use_cli": true, 
  "default_symbol": "CL=F",
  "default_timeframe": "1d",
  "return_column": "daily_return_%",
  "debug_mode": false,
  "data_source": "mt5",
  "show_plots": false,


  "outlier_filtering": {
      "method": "combined",
      "z_thresh": 3.0,
      "lower_quantile": 0.01,
      "upper_quantile": 0.99,
      "audit_filename": "data_excluded_outliers.csv"
	    },

  "state_classification": {
    "std_cap_multiplier": 0.4,
    "intraday_bias": {
        "default": 1.0,
        "intraday": 0.6
    }
  },

  "conditional_probability_state_threshold": 0.1,
  "max_chain_length": 5,
  "state_columns_annotated": ["state", "candle_state", "candlestick", "gain_loss"],
  "state_column": "state",

  "hmm_n_states": 2,
  "var_cvar_confidance_level": 0.95,

  "monte_carlo": {
      "n_steps": 10,
      "n_simulations": 500
	    },

  "risk_per_trade": 0.001,
  "account_size": 10000,

  "symbol": {
	      "CL=F": {
	      "name":"Crude Oil Future",
	      "contract_size": 100,
	      "tick_size": 0.01,
	      "multiplier": 1},

	      "default": {
		"name":"Unknown",
		"contract_size": 1,
		"tick_size": 0.01,
		"multiplier": 1},

	      "GC=F": {    
		"name": "Gold Futures",
		"contract_size": 100,
		"tick_size": 0.1,
		"multiplier": 10}
	    }

}
